#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/opt/agent/temp/buildTmp/base-starter-flow-karaf/starter/node_modules/.pnpm/atob@2.1.2/node_modules/atob/bin/node_modules:/opt/agent/temp/buildTmp/base-starter-flow-karaf/starter/node_modules/.pnpm/atob@2.1.2/node_modules/atob/node_modules:/opt/agent/temp/buildTmp/base-starter-flow-karaf/starter/node_modules/.pnpm/atob@2.1.2/node_modules:/opt/agent/temp/buildTmp/base-starter-flow-karaf/starter/node_modules/.pnpm/node_modules:/opt/agent/temp/buildTmp/base-starter-flow-karaf/starter/node_modules:/opt/agent/temp/buildTmp/base-starter-flow-karaf/node_modules:/opt/agent/temp/buildTmp/node_modules:/opt/agent/temp/node_modules:/opt/agent/node_modules:/opt/node_modules:/node_modules:/opt/agent/temp/buildTmp/base-starter-flow-karaf/starter/node_modules/atob/bin/node_modules:/opt/agent/temp/buildTmp/base-starter-flow-karaf/starter/node_modules/atob/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../atob/bin/atob.js" "$@"
else
  exec node  "$basedir/../atob/bin/atob.js" "$@"
fi
