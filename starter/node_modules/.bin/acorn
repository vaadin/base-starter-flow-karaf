#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/opt/agent/temp/buildTmp/base-starter-flow-karaf/starter/node_modules/.pnpm/acorn@6.4.2/node_modules/acorn/bin/node_modules:/opt/agent/temp/buildTmp/base-starter-flow-karaf/starter/node_modules/.pnpm/acorn@6.4.2/node_modules/acorn/node_modules:/opt/agent/temp/buildTmp/base-starter-flow-karaf/starter/node_modules/.pnpm/acorn@6.4.2/node_modules:/opt/agent/temp/buildTmp/base-starter-flow-karaf/starter/node_modules/.pnpm/node_modules:/opt/agent/temp/buildTmp/base-starter-flow-karaf/starter/node_modules:/opt/agent/temp/buildTmp/base-starter-flow-karaf/node_modules:/opt/agent/temp/buildTmp/node_modules:/opt/agent/temp/node_modules:/opt/agent/node_modules:/opt/node_modules:/node_modules:/opt/agent/temp/buildTmp/base-starter-flow-karaf/starter/node_modules/acorn/bin/node_modules:/opt/agent/temp/buildTmp/base-starter-flow-karaf/starter/node_modules/acorn/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../acorn/bin/acorn" "$@"
fi
